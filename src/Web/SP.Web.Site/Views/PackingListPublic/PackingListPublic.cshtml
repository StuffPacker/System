@model string
@{
    Layout = "_Layout";
    
}
<div id="PackingListPublic">
    <h1 data-bind="text:Name" class="page-header">List</h1>
     <a title="Printable" style="font-size:large;" data-bind="attr:{href:PrintLink}" target="_blank" class="glyphicon glyphicon-print" aria-hidden="true"></a>
    <span data-bind="text:Description"></span>
    <br/><br/>
    
    <!-- ko foreach: Groups -->
     @await Html.PartialAsync("PackinglistGroup")
    <!-- /ko -->
</div> 

@section footerjs
{
    <script type="text/javascript">  
  ko.applyBindings(PackingListPublicViewModel('@Model'), document.getElementById('PackingListPublic'));  
  </script>
}